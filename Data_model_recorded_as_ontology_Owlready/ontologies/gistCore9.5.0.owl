		<rdfs:domain rdf:resource="&gist;UnitOfMeasure"/>
		<rdfs:isDefinedBy rdf:resource="https://ontologies.semanticarts.com/o/gistCore"/>
		<rdfs:range rdf:resource="&gist;CoherentUnit"/>
		<skos:definition rdf:datatype="&xsd;string">For a complex unit refers to a unit that has all the component parts in SI</skos:definition>
		<skos:prefLabel rdf:datatype="&xsd;string">Has Standard Unit</skos:prefLabel>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&gist;hasStreetAddress">
		<rdfs:isDefinedBy rdf:resource="https://ontologies.semanticarts.com/o/gistCore"/>
