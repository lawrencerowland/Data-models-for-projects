		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<owl:Restriction>
						<owl:onProperty rdf:resource="&gist;hasPrecision"/>
						<owl:someValuesFrom rdf:resource="&gist;Magnitude"/>
					</owl:Restriction>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&gist;hasUoM"/>
