		<skos:prefLabel rdf:datatype="&xsd;string">Has Altitude</skos:prefLabel>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&gist;hasBaseUnit">
		<rdfs:subPropertyOf rdf:resource="&gist;hasStandardUnit"/>
		<rdfs:domain rdf:resource="&gist;UnitOfMeasure"/>
		<rdfs:isDefinedBy rdf:resource="https://ontologies.semanticarts.com/o/gistCore"/>
